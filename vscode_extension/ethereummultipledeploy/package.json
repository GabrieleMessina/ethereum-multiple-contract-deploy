{
  "name": "ethereummultipledeploy",
  "displayName": "Ethereum Contracts Multiple Deploy",
  "description": "Deploy multiple contract easily",
  "author": "Gabriele Messina",
  "publisher": "GabrieleMessina",
  "version": "1.0.2",
  "license": "MIT",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "homepage": "https://github.com/GabrieleMessina/remix-multiple-contract-deploy",
    "repository": {
        "type": "git",
        "url": "https://github.com/GabrieleMessina/remix-multiple-contract-deploy"
    },
  "contributes": {
    "commands": [
      {
        "command": "ethereummultipledeploy.deployMultipleContracts",
        "title": "Solidity: Deploy Multiple Contracts"
      }
    ],
    "views": {
      "debug": [
        {
          "id": "ethereummultipledeploy.deployView",
          "type": "webview",
          "name": "Deploy Contracts"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.98.0",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "antlr4ng": "^3.0.16",
    "solc": "^0.8.28",
    "web3": "^4.16.0"
  }
}
